{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "esnext"],
    "jsx": "react",
    "module": "es6",
    "moduleResolution": "node",
    "baseUrl": "./",
    "strict": true,
    "esModuleInterop": true,
    "noImplicitAny": false

  },
  "exclude": [
    "node_modules",
    "lib",
    "**/*.d.ts"
  ],
  "include": [
    "src/*",
    "src/custom.d.ts"
  ],
}
